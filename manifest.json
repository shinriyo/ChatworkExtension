{
    "name": "ChatWork Ext",
    "description": "未読消す。",
    "version": "0.1",
    "manifest_version": 2,
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
        "tabs", "https://www.chatwork.com/*"
    ],
    "content_scripts": [
        {
            "matches": ["https://www.chatwork.com/*"],
            "js": ["js/jquery-2.1.4.min.js"]
        }
    ],
    "browser_action": {
        "default_icon": "icon19.png",
        "default_title": "ChatWork Ext"
    }
}
